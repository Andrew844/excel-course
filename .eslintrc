{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "quotes": [2, "double", {
      "avoidEscape": true
    }],
    "operator-linebreak": "off",
    "linebreak-style": "off"
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended", "google"
  ]
}
